Standings:
<br />
<table class="table">
	<tr>
		<th> Rank </th>
		<th> Email </th>
		<th> Win % </th>
	</tr>
	<% rank = 0 %>
<% User.sorted_by_percentage_correct.each do |u| %>
	<% rank += 1 %>
	<tr>
		<td> <%= rank %> </td>
		<td> <%= link_to u.email, u %> </td>
		<td> <%= u.percentage_correct %> </td>
	</tr>
<% end %>
</table>


